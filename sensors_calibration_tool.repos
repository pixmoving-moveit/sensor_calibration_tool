repositories:
# ros2 
  # camera intrinsic calibration
  calibration_ws/src/image_pipeline:
    type: git
    url: https://github.com/ros-perception/image_pipeline.git
    version: humble
  # lidar2lidar
  calibration_ws/src/calibration_tools:
    type: git
    url: https://github.com/pixmoving-moveit/calibration_tools.git
    version: feature/ros2-calibration-tools
  # imu intrinsic calibration
  calibration_ws/src/imu_utils:
